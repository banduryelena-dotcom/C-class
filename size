};
   std::cout<<"\n"<< barsik.image;
   
 std::vector<Cat> cats = std::vector<Cat> {
    { 
    "barsik",
    100,
    "https://i.natgeofe.com/n/548467d8-c5f1-4551-9f58-6817a8d2c45e/NationalGeographic_2572187_16x9.jpg"
