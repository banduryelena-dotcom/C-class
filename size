#include <iostream>
#include <vector>
#include <unordered_map>

struct Cat {
    std::string name;
    int price;
   std::string image;
} ;

void printItemsAsJson(std::vector<Cat> itemsArray) {
   std::cout << "[" << std::endl;
    for (const Cat item : itemsArray) {
        std::cout << "  {" << std::endl;
        std::cout << "    \"name\": \"" << item.name << "\"," << std::endl;
        std::cout << "    \"price\": " << item.price << "," << std::endl;
        std::cout << "    \"image\": \"" << item.image << "\"" << std::endl;
        std::cout << "  }," << std::endl;
    }
    std::cout << "]" << std::endl;
}

int main()
{
    
  std::vector<int> numbers = std::vector<int> {1,2,3,4,5,6};
  
  for(int i = numbers.size() -1; i>=0; i--){
  std::cout<< numbers[i];
}

Cat barsik =  {
